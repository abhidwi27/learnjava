package array;

public class ReaaragneOddEven {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a[] = {1, 2, 3, 4, 5, 6, 7};
		int j, temp;
		for( int i = 1; i< a.length; i++){
			j = i;
			while( j > 0 ){
			
				if(i%2 == 0){			
						if(a[i] < a[i-2]){
							temp = a[i-2];
							a[i-2] = a[i];
							a[i] = temp;
							
						}
					
				}else{
					
						
					
				
				}
			}
		}
		ArrayPrint.printArray(a);

	}

}
