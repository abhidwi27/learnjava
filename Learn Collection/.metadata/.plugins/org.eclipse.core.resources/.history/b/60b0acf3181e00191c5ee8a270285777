package multithreading;

public class LearnIncAndDec {
	
	static int  i = 0;
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		
		Thread t1 = new Thread( new Runnable(){
			
			@Override
			public void run() {
				// TODO Auto-generated method stub
				i++;
			}
		}, "first");
		
		Thread t2 = new Thread(){
			@Override
			public void run() {
				// TODO Auto-generated method stub
				i++;
			}
			
		}, "" );

	}

}
