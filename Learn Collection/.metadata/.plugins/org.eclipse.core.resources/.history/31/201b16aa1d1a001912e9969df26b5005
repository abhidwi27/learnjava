	System.out.println( "Array Deque Examples!" );

    Deque<String> stack = new ArrayDeque<String>();
    stack.push("first");
    stack.push("second");
    System.out.println("stack pollFirst = "+stack.pollFirst());
    System.out.println("stact pollLast = "+stack.pollLast());
    System.out.println("stack peek = "+stack.peek());

    Deque<String> queue = new ArrayDeque<String>();
    queue.offer("one");
    queue.offer("two");

    System.out.println("queue pollFirst = "+queue.pollFirst());
    System.out.println("queue pollLast = "+queue.pollLast());
    System.out.println("queue peek = "+queue.peek());
}
